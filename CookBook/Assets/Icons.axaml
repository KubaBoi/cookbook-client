    <Design.PreviewWith>
        <Border>
            <DockPanel>
                <Path Width="30"
                      Height="30"
                      Data="{StaticResource MinusIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
                <TextBlock DockPanel.Dock="Top"
                           Text="MinusIcon_Path" />

                <Path Width="30"
                      Height="30"
                      Data="{StaticResource PlusIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
                <TextBlock DockPanel.Dock="Top"
                           Text="PlusIcon_Path" />

                <!-- Timer control icons -->
                <Path Width="30"
                      Height="30"
                      Data="{StaticResource DownBigIcon_Path}"
