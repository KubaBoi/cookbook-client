                           Text="UpBigIcon_Path" />

                <Path Width="30"
                      Height="30"
                      Data="{StaticResource UpSmallIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
                <TextBlock DockPanel.Dock="Top"
                           Text="UpSmallIcon_Path" />

            </DockPanel>
        </Border>
    </Design.PreviewWith>

