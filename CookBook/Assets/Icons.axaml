                           Text="DownBigIcon_Path" />

                <Path Width="30"
                      Height="30"
                      Data="{StaticResource DownSmallIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
