                           Text="DownSmallIcon_Path" />

                <Path Width="30"
                      Height="30"
                      Data="{StaticResource PauseIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
                <TextBlock DockPanel.Dock="Top"
                           Text="PauseIcon_Path" />

                <Path Width="30"
                      Height="30"
                      Data="{StaticResource UpBigIcon_Path}"
                      DockPanel.Dock="Top"
                      Stroke="Red"
                      StrokeThickness="1" />
